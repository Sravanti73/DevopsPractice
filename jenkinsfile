node{
    def app
    stage('clone from github'){
        checkout scm
    }
    stage('build'){
        app=docker.build("sravanti73/helloworld-docker")
    }
    #stage('test'){
     #   app.run("-w /Jenkinsdata/Jenkins/.jenkins/workspace/Test_Pipeline/").inside(){
      #      sh 'echo "test passed"'
        
     # }
    #}
    stage('push image'){
        docker.withregistry('https://registry.hub.docker.com', 'dockerhub-cred'){
            app.push("${env.Build_Number}")
            app.push("latest")
        }
    }
}
